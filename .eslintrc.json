{
  "extends": "next/core-web-vitals",
  "rules": {
    "@typescript-eslint/no-confusing-void-expression": "off",
    "semi": ["error", "never"],
    "quotes": ["error", "single"],
    "comma-dangle": ["error", "never"],
    "no-unused-vars": [
      "error",
      {
        "varsIgnorePattern": "^_",
        "argsIgnorePattern": "^_"
      }
    ],
    "space-before-function-paren": ["error", "always"]
  }
}
